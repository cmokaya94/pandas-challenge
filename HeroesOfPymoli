    "u_item = purchase_data[\"Item ID\"].unique()\n",
    "f_item = len(u_item)\n",
    "\n",
    "\n",
    "avg = purchase_data[\"Price\"].mean()\n",
    "\n",
    "\n",
    "total_purchases = purchase_data[\"Purchase ID\"]\n",
    "total = len(total_purchases)\n",
    "\n",
    "revenue = purchase_data[\"Price\"].sum()\n",
    "revenue\n",
    "\n",
    "summary_df = pd.DataFrame([{\"Number of Unique Items\" : f_item,\n",
    "                          \"Average Price\": avg,\"Number of Purchases\": total,\"Total Revenue\":revenue}])\n",
    "summary_df[\"Average Price\"] = summary_df[\"Average Price\"].map(\"${:,.2f}\".format)\n",
    "summary_df[\"Total Revenue\"] = summary_df[\"Total Revenue\"].map(\"${:,.2f}\".format)\n",
    "summary_df"
