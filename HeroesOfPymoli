   "source": [
    "bins = [0,9,14,19,24,29,34,39,100]\n",
    "label = [\"<10\",\"10-14\",\"15-19\",\"20-24\",\"25-29\",\"30-34\",\"35-39\",\"40+\"]\n",
    "new_data = purchase_data.drop_duplicates(subset = \"SN\")\n",
    "new_data[\"new\"] = pd.cut(new_data[\"Age\"],bins,labels=label)\n",
    "\n",
    "total=len(new_data)\n",
    "\n",
    "l_10=len(new_data.loc[new_data[\"Age\"] < 10])\n",
    "l_15=len(new_data.loc[(new_data['Age'] >= 10) & (new_data['Age'] < 15)])\n",
    "l_20=len(new_data.loc[(new_data['Age'] >= 15) & (new_data['Age'] < 20)])\n",
    "l_25=len(new_data.loc[(new_data['Age'] >= 20) & (new_data['Age'] < 25)])\n",
    "l_30=len(new_data.loc[(new_data['Age'] >= 25) & (new_data['Age'] < 30)])\n",
    "l_35=len(new_data.loc[(new_data['Age'] >= 30) & (new_data['Age'] < 35)])\n",
    "l_40=len(new_data.loc[(new_data['Age'] >= 35) & (new_data['Age'] < 40)])\n",
    "l_45=len(new_data.loc[new_data[\"Age\"] >= 40])\n",
    "\n",
    "p_10=(l_10/total)*100\n",
    "p_15=(l_15/total)*100\n",
    "p_20=(l_20/total)*100\n",
    "p_25=(l_25/total)*100\n",
    "p_30=(l_30/total)*100\n",
    "p_35=(l_35/total)*100\n",
    "p_40=(l_40/total)*100\n",
    "p_45=(l_45/total)*100\n",
    "\n",
    "total_count = {\n",
    "   \"Total:\": [l_10,l_15,l_20,l_25,l_30,l_35,l_40,l_45],\n",
    "    \"Percentage\":[p_10,p_15,p_20,p_25,p_30,p_35,p_40,p_45]\n",
    "}\n",
    "\n",
    "total_df = pd.DataFrame(total_count,index=label)\n",
    "total_df[\"Percentage\"] = total_df[\"Percentage\"].map(\"{:,.2f}%\".format)\n",
    "total_df\n",
    "\n"
   ]
  },
