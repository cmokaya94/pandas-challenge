   "source": [
    "bins = [0,9,14,19,24,29,34,39,100]\n",
    "label = [\"<10\",\"10-14\",\"15-19\",\"20-24\",\"25-29\",\"30-34\",\"35-39\",\"40+\"]\n",
    "purchase_data[\"Age Range\"]  = pd.cut(purchase_data[\"Age\"],bins,labels=label)\n",
    "\n",
    "age_group = purchase_data.groupby(\"Age Range\")\n",
    "count_1 =purchase_data[\"Age Range\"].value_counts()\n",
    "count_2 =age_group [\"Price\"].mean()\n",
    "count_3 =age_group [\"Price\"].sum()\n",
    "total_count=purchase_data.groupby(\"Age Range\")[\"SN\"].nunique()\n",
    "\n",
    "avg_tat = count_3/total_count\n",
    "avg_tat\n",
    "\n",
    "sumet = pd.DataFrame({\n",
    "  \"Purchase Count\":count_1,\n",
    "  \"Average Purchase Price\": count_2,\n",
    "    \"Total Purhcase Value\":count_3,\n",
    "    \"Average Purchase Per Person\": avg_tat})\n",
    "\n",
    "sumet[\"Average Purchase Price\"] = sumet[\"Average Purchase Price\"].map(\"${:,.2f}\".format)\n",
    "sumet[\"Total Purhcase Value\"] = sumet[\"Total Purhcase Value\"].map(\"${:,.2f}\".format)\n",
    "sumet[\"Average Purchase Per Person\"] = sumet[\"Average Purchase Per Person\"].map(\"${:,.2f}\".format)\n",
    "\n",
    "sumet\n",
    "\n",
    "\n"
   ]
