    "male=purchase_data[purchase_data[\"Gender\"] == \"Male\"]\n",
    "male_1 = len(male[\"SN\"].unique())\n",
    "\n",
    "female=purchase_data[purchase_data[\"Gender\"] == \"Female\"]\n",
    "female_1 = len(female[\"SN\"].unique())\n",
    "\n",
    "other=purchase_data[purchase_data[\"Gender\"] == \"Other / Non-Disclosed\"]\n",
    "other_1 = len(other[\"SN\"].unique())\n",
    "\n",
    "percent_male = male_1/total_players * 100\n",
    "percent_female = female_1/total_players *100\n",
    "percent_other = other_1/total_players * 100\n",
    "\n",
    "gender = { \"Total Count\": [male_1,female_1,other_1],\n",
    "          \"Percentage of Players\": [percent_male,percent_female,percent_other]}\n",
    "summary_gender = pd.DataFrame(gender, index = ['Male', \"Female\",\"Other/ Non-Disclosed\"])\n",
    "summary_gender[\"Percentage of Players\"] = summary_gender[\"Percentage of Players\"].map(\"{:,.2f}%\".format)\n",
    "summary_gender\n",
    "\n",
